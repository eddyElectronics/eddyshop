{
  "name": "eddyshop",
  "version": "1.0.0",
  "private": true,
  "description": "Production-ready e-commerce SPA built with Next.js",
  "author": "EddyElectronics",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "pnpm lint && pnpm type-check && pnpm test:ci",
    "clean": "rm -rf .next node_modules coverage",
    "prepare": "echo 'Ready for production'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "@vercel/blob": "^2.0.0",
    "@vercel/kv": "^3.0.0",
    "next": "16.1.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "jsdom": "^24.1.3",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^1.6.1"
  }
}
